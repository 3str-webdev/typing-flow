{
  "name": "typing-flow",
  "version": "0.1.17",
  "description": "Npm package for typing animation for web",
  "keywords": [
    "typing",
    "flow",
    "typing-flow",
    "es6",
    "animation",
    "utility",
    "npm",
    "node",
    "web",
    "typescript"
  ],
  "author": "sergey-frey",
  "repository": {
    "type": "git",
    "url": "https://github.com/sergey-frey/typing-flow"
  },
  "homepage": "https://typing-flow.gitbook.io/docs",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./commands": {
      "require": "./dist/commands.cjs",
      "import": "./dist/commands.js",
      "types": "./dist/commands.d.ts"
    },
    "./renderers": {
      "require": "./dist/renderers.cjs",
      "import": "./dist/renderers.js",
      "types": "./dist/renderers.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev:ui": "vite",
    "dev:lib": "vite build --watch",
    "dev:test": "vitest",
    "dev": "concurrently \"npm:dev:ui\" \"npm:dev:lib\" \"npm:dev:test\"",
    "formate": "prettier . --write",
    "build": "vite build",
    "prepare": "husky",
    "test": "vitest run"
  },
  "devDependencies": {
    "@types/node": "^22.5.2",
    "concurrently": "^9.1.1",
    "happy-dom": "^16.5.2",
    "husky": "^9.1.6",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^4.1.0",
    "vitest": "^2.1.8"
  }
}
